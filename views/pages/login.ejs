<!DOCTYPE html>
<html>
<head>
	<title>Survey</title>
	<%- include('../asset/meta') %>		
	<%- include('../asset/styles_main') %>		
	<%- include('../asset/scripts_main') %>
	<link rel="stylesheet" type="text/css" href="<%=meta.cdn_url%>/login/login.css"/>
	<script type="text/javascript" src="<%=meta.cdn_url%>/login/login.js"></script>
</head>
<body>
	<%- include('../partials/page_waiting') %>		
	<div class="containerMain">
		<div class="img">
			<img src="<%=meta.cdn_url%>/img/oja_cover.png">
		</div>
		<div class="login-content">
			<form class="a form app" id="login_form" method="post" action="/signin/login">
				<img class="imgLogo" src="<%=meta.cdn_url%>/img/logoLaw.png">
				<h2 class="title">Welcome</h2>
           		<div class="input-div one">
           		   <div class="i">
           		   		<i class="fa fa-user"></i>
           		   </div>
           		   <div class="div">
           		   		<h5>Username</h5> 
           		   		<input type="text" id="username" class="input" name="username" tabindex="1">
           		   </div>
           		</div>
           		<div class="input-div pass">
           		   <div class="i"> 
           		    	<i class="fa fa-lock"></i>
           		   </div>
           		   <div class="div">
           		    	<h5>Password</h5>
           		    	<input type="password" id="password" class="input" name="password" tabindex="2"> 
            	   </div>
            	</div>
            	<button class="butn btn-form-login" id="buttonlogin" tabindex="3">Login</button>
				<div id="versionlayer">v1.01</div>
            </form>
        </div>
    </div>
</body>
</html>
